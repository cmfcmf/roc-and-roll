  "jest": {
    "testEnvironment": "jest-environment-jsdom",
    "transform": {
      "^.+\\.[jt]sx?$": [
        "@swc/jest",
        {
          "module": {
            "type": "commonjs"
          },
          "sourceMaps": true
        }
      ],
      "^.+\\.peggy$": "<rootDir>/jest-peggy-transform.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.ts"
    ],
    "globals": {
      "__VERSION__": "main"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "src/(.*)": "<rootDir>/src/$1",
      "^.+\\.(css|less|scss|mp3|svg|png)$": "identity-obj-proxy"
    },
    "roots": [
      "<rootDir>/src/client",
      "<rootDir>/src/server",
      "<rootDir>/src/shared"
    ]
  },
  "lint-staged": {
    "*": [
      "prettier --write --no-error-on-unmatched-pattern --ignore-unknown --cache --cache-strategy content",
      "eslint   --fix   --no-error-on-unmatched-pattern                  --cache --cache-strategy content"
    ]
  },
  "engines": {
    "node": "16.x",
    "yarn": ">=3"
  },
