  "browserslist": "defaults",
  "jest": {
    "testEnvironment": "jest-environment-jsdom",
    "transform": {
      "^.+\\.[jt]sx?$": [
        "@swc/jest",
        {
          "module": {
            "type": "commonjs"
          },
          "sourceMaps": true
        }
      ]
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.ts"
    ],
    "globals": {
      "__VERSION__": "master"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "src/(.*)": "<rootDir>/src/$1",
      "^.+\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "roots": [
      "<rootDir>/src"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix"
    ]
  },
  "engines": {
    "node": ">=14",
    "yarn": ">=3"
  },
  "resolutions": {
    "@reduxjs/toolkit": "patch:@reduxjs/toolkit@npm:1.6.1#.yarn/patches/@reduxjs-toolkit-npm-1.6.1-a0999c8262"
  },
  "packageManager": "yarn@3.1.0-rc.2"
}
