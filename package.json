    "webpack-dev-server-waitpage": "^2.1.1",
    "webpack-node-externals": "^3.0.0"
  },
  "browserslist": "defaults",
  "jest": {
    "testEnvironment": "jest-environment-jsdom",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "setupFiles": [],
    "globals": {
      "__VERSION__": "master",
      "ts-jest": {
        "tsconfig": "tsconfig.json",
        "babelConfig": {
          "presets": [
            [
              "@babel/preset-env",
              {
                "useBuiltIns": "entry",
                "corejs": 3,
                "targets": {
                  "node": 14
                }
              }
            ],
            "@babel/preset-react",
            "@babel/preset-typescript"
          ]
        }
      }
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "src/(.*)": "<rootDir>/src/$1",
      "^.+\\.(css|less|scss)$": "babel-jest"
    },
    "roots": [
      "<rootDir>/src"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "engines": {
    "node": ">=14",
    "yarn": ">=2"
  }
}
