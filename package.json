  "jest": {
    "testEnvironment": "jest-environment-jsdom",
    "transform": {
      "^.+\\.[jt]sx?$": [
        "@swc/jest",
        {
          "module": {
            "type": "commonjs"
          },
          "sourceMaps": true
        }
      ],
      "^.+\\.peggy$": "<rootDir>/jest-peggy-transform.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.ts"
    ],
    "globals": {
      "__VERSION__": "main"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "src/(.*)": "<rootDir>/src/$1",
      "^.+\\.(css|less|scss|mp3|svg|png)$": "identity-obj-proxy"
    },
    "roots": [
      "<rootDir>/src/client",
      "<rootDir>/src/server",
      "<rootDir>/src/shared"
    ]
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown --no-error-on-unmatched-pattern",
      "eslint --cache --cache-strategy content --fix --no-error-on-unmatched-pattern"
    ]
  },
