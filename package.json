{
  "private": true,
  "name": "roc-and-roll",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "dev": "concurrently --handle-input --kill-others --names server,client --prefix-colors green,blue yarn:dev:server yarn:dev:client",
    "dev:client": "webpack serve   --config webpack.client.js --env development",
    "dev:server": "webpack --watch --config webpack.server.js --env development",
    "test": "jest",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src",
    "format": "yarn run lint --fix",
    "build": "concurrently --names client,server --prefix-colors blue,green yarn:build:client yarn:build:server",
    "build:client": "webpack --config webpack.client.js --env production",
    "build:server": "webpack --config webpack.server.js --env production",
    "analyze-bundle:client": "webpack --config webpack.client.js --env production --profile --json dist/client/stats.json && yarn webpack-bundle-analyzer dist/client/stats.json",
    "start": "nodemon --watch dist/server.roc-and-roll.js dist/server.roc-and-roll.js --workspace ./workspace",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@doodle3d/clipper-js": "^1.0.11",
